{
  "expo": {
    "name": "OnboardingApp",
    "slug": "OnboardingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/firstcl.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maheshboopathiap.OnboardingApp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby places and services.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to show nearby places and services."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/firstcl.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.maheshboopathiap.OnboardingApp",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "This app needs access to your location to show nearby places and services.",
          "locationAlwaysAndWhenInUsePermission": "This app needs access to your location to show nearby places and services.",
          "isIosBackgroundLocationEnabled": false,
          "isAndroidBackgroundLocationEnabled": false
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "55dcea56-5f5d-4d6d-86c6-96eae0649165"
      }
    }
  }
}